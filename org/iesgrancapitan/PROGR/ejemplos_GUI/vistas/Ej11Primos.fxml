      <Label text="Cantidad de numeros primos a mostrar" />
      <TextField fx:id="cantidad" prefHeight="25.0" prefWidth="79.0" GridPane.columnIndex="1" />
      <TextArea fx:id="resultado" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1" />
      <Button mnemonicParsing="false" onAction="#calcula" text="Calcular" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
